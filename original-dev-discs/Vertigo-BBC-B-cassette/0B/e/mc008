		TTL	mc008		; Data for nasty's screen route*****************************************	Bits 1-4 = Speed ( Delay )	**	=0-7	N.B. 0 = still		******************************************	Bits 8-32 = Number of blocks	**	=1-7				******************************************	Bits 64-128 = Direction,	**	=0-3	0 = NW, 1 = NE,		**		2 = SW, 3 = SE.		******************************************					** SYNTAX : 	MOVE	SPEED,MOVES,DIRECTION*		START	X,Y	( 2 bytes each )*					*****************************************nasty_lookup	DW	S0L0		; Level	1		DW	S2L1		DW	S3L1		DW	S4L1		DW	S5L1		DW	S6L1 		DW	S7L1		DW	S8L1		DW	S0L0		DW	S10L1		DW	S1L2		; Level 2		DW	S2L2		DW	S3L2		DW	S4L2		DW	S5L2		DW	S0L0		DW	S7L2		DW	S0L0		DW	S3L2		DW	S0L0		DW	S1L3		; Level 3		DW	S0L0		DW	S0L0		DW	S0L0		DW	S5L3		DW	S0L0		DW	S7L3		DW	S8L3		DW	S0L0		DW	S10L3		DW	S0L0		; Level 4		DW	S2L4		DW	S3L4		DW	S4L4		DW	S5L4		DW	S6L4		DW	S9L5		DW	S0L0		DW	S0L0		DW	S0L0		DW	S0L0		; Level 5		DW	S2L5		DW	S3L5		DW	S9L5		DW	S5L5		DW	S0L0		DW	S0L0		DW	S8L5		DW	S9L5		DW	S0L0****************************************	; Level 0 data****************************************S0L0		START	1,1		; No nasty !!!****************************************	; Level 1 data****************************************S2L1		START	416,416		MOVE	7,3,NW		MOVE	7,5,SW		MOVE	7,3,SE		MOVE	7,5,NE		DFB	0S3L1		START	32,544		MOVE	7,7,SW		MOVE	7,1,SW		MOVE	7,7,SE		MOVE	6,1,SE		MOVE	7,2,NE		MOVE	7,1,SE		MOVE	7,7,NE		MOVE	7,7,NW		MOVE	7,2,NW		MOVE	7,1,SW		DFB	0S4L1		START	224,352		MOVE	7,2,SE		MOVE	7,1,NE		MOVE	7,1,NW		MOVE	7,3,SW		MOVE	7,1,SE		MOVE	6,1,SE		MOVE	7,2,NW		MOVE	6,1,NW		MOVE	7,2,SE		MOVE	7,3,NE		MOVE	7,1,NW		MOVE	7,1,SW		MOVE	7,1,SE		MOVE	6,1,SE		MOVE	7,2,NW		MOVE	6,1,NW		DFB	0S5L1		START	160,608		MOVE	2,1,SW		MOVE	7,1,NE		MOVE	7,6,SE		MOVE	7,1,SW		MOVE	7,1,SE		MOVE	7,4,SW		MOVE	7,7,NW		MOVE	7,5,NE		MOVE	7,2,NW		MOVE	7,6,SW		MOVE	7